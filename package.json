{
	"name": "iptv",
	"version": "1.0.0",
	"description": "IPTV channel generator",
	"main": "index.js",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"start": "node server.js",
		"dev": "live-server docs --port=3000",
		"test": "vitest",
		"format": "biome format --write .",
		"format:all": "biome check --write .",
		"format:check": "biome check .",
		"lint": "biome lint ."
	},
	"bin": {
		"generate": "./bin/generate.js",
		"check": "./bin/check.js"
	},
	"author": "",
	"license": "Apache-2.0",
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"live-server": "^1.2.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"iptv-util": "^1.3.0"
	}
}
